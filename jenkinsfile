pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
               git branch: 'main', credentialsId: 'krish', url: 'https://github.com/krishnarju/onlyme.git'
            }
        }
        stage('build') {
            steps {
               sh 'mvn -B -DskipTests clean package'
            }
        }
                        

   } // End of Sonar Scan
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
