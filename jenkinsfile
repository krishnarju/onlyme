pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
               git branch: 'main', credentialsId: 'krish', url: 'https://github.com/krishnarju/onlyme.git'
            }
        }
        stage('build') {
            steps {
               sh 'mvn -B -DskipTests clean package'
            }
        }
        
        
        stage('SonarQube analysis') {
        steps{
        withSonarQubeEnv('sonarqube-8.9') { 
        sh "mvn sonar:sonar"
    }
        }
        }
                      
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
